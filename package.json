{
  "name": "callme",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env PORT=4057 node server.js",
    "build": "next build",
    "start": "cross-env NODE_ENV=production PORT=4057 node server.js",
    "start:render": "cross-env NODE_ENV=production PORT=10000 HOSTNAME=0.0.0.0 node server-render.js",
    "lint": "eslint",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:rebuild": "docker-compose down && docker-compose build && docker-compose up -d"
  },
  "dependencies": {
    "next": "16.0.3",
    "peerjs": "^1.5.5",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/peerjs": "^0.0.30",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
